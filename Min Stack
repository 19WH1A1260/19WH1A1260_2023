class MinStack {
public:
    MinStack() {
        stack<int>stk;
    }
    
    void push(int val) {
        stk.push(val);
        return;
    }
    
    void pop() {
        if(!stk.empty())
        {
            stk.pop();
        }
    }
    
    int top() {
        if(!stk.empty())
        {
            stk.top();
        }
    }
    
    int getMin() {
        
    }
};
